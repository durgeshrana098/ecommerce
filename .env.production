APP_NAME=Bagisto
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-app-name.onrender.com
APP_ADMIN_URL=admin
APP_TIMEZONE=UTC

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_CURRENCY=USD

LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=error

# Database (will be auto-configured by Render)
DB_CONNECTION=mysql
DB_HOST=
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=public
QUEUE_CONNECTION=sync

CACHE_STORE=file
CACHE_PREFIX=

# Redis (will be auto-configured by Render)
REDIS_CLIENT=phpredis
REDIS_HOST=
REDIS_PASSWORD=
REDIS_PORT=6379

RESPONSE_CACHE_ENABLED=true

# Mail Configuration (Update with your SMTP settings)
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_ENCRYPTION=tls

MAIL_FROM_ADDRESS=noreply@yourdomain.com
MAIL_FROM_NAME="Bagisto Store"

ADMIN_MAIL_ADDRESS=admin@yourdomain.com
ADMIN_MAIL_NAME=Admin

CONTACT_MAIL_ADDRESS=contact@yourdomain.com
CONTACT_MAIL_NAME=Contact

# AWS S3 (Optional - for file storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
